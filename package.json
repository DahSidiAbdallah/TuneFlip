{
  "name": "tuneflip",
  "version": "0.1.0",
  "private": true,
  "description": "TuneFlip: Premium-quality bulk video to MP3 converter with advanced controls (bitrate, VBR, loudness, trim, concurrency).",
  "license": "MIT",
  "type": "module",
  "bin": {
    "vid2mp3": "./dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p . && node ./dist/gui/copy-assets.js",
    "dev": "ts-node --transpile-only src/index.ts --help",
    "start": "node dist/index.js",
    "gui": "electron ./dist/gui/main.js",
    "lint": "echo 'No linter configured'"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "electron": "^30.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
